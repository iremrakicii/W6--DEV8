CREATE TABLE employee(
id INTEGER PRIMARY KEY,
name VARCHAR(50) NOT NULL,
birthday DATE
email VARCHAR(100),
);

UPDATE employee
SET name='Mary',
    email='maryko@gmail.com',
	birthday= '1950-07-17'
WHERE id=40;

UPDATE employee
SET name='Jane',
    email='janeemo@gmail.com',
	birthday= '1960-09-19'
WHERE id=15;

UPDATE employee
SET name='Harry',
    email='harrymarr@gmail.com',
	birthday= '1970-05-18'
WHERE id=20;

UPDATE employee
SET name='Curly',
    email='curlycoo@gmail.com',
	birthday= '1980-03-12'
WHERE id=30;

UPDATE employee
SET name='Jerry',
    email='tomjerryy@gmail.com',
	birthday= '1960-06-16'
WHERE id=25;

DELETE FROM employee
WHERE id=5;

DELETE FROM employee
WHERE id=10;

DELETE FROM employee
WHERE id=15;

DELETE FROM employee
WHERE id=20;

DELETE FROM employee
WHERE id=25;
